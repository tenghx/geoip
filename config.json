{
    "input": [
        {
            "type": "text",
            "action": "add",
            "args": {
                "name": "cn",
                "uri": "https://raw.githubusercontent.com/v2fly/geoip/release/text/cn.txt"
            }
        },
        {
            "type": "cutter",
            "action": "remove",
            "args": {
                "wantedList": ["cn"],
                "onlyIPType": "ipv4"
            }
        },
        {
            "type": "text",
            "action": "add",
            "args": {
                "name": "cn",
                "uri": "https://raw.githubusercontent.com/17mon/china_ip_list/master/china_ip_list.txt",
                "onlyIPType": "ipv4"
            }
        },
        {
            "type": "text",
            "action": "add",
            "args": {
                "name": "hk",
                "uri": "https://raw.githubusercontent.com/v2fly/geoip/release/text/hk.txt"
            }
        },
        {
            "type": "text",
            "action": "add",
            "args": {
                "name": "mo",
                "uri": "https://raw.githubusercontent.com/v2fly/geoip/release/text/mo.txt"
            }
        },
        {
            "type": "private",
            "action": "add"
        }
    ],
    "output": [
        {
            "type": "v2rayGeoIPDat",
            "action": "output",
            "args": {
                "outputName": "geoip.dat",
                "outputDir": "./publish"
            }
        }
    ]
}
